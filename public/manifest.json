

{
  "manifest_version": 3,
  "name": "Selective Cache Clearer",
  "version": "1.0",
  "description": "Clear cache for specific applications without clearing all browser data.",
  "permissions": [
    "browsingData",
    "activeTab",
    "tabs",  
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Selective Cache Clearer"
  }
}

