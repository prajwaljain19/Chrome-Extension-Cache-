{
  "manifest_version": 3,
  "name": "Selective Cache Clearer",
  "version": "1.0",
  "description": "Clear cache for specific applications without clearing all browser data.",
  "permissions": [
    "browsingData",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon.ico",
      "48": "favicon.ico",
      "128": "favicon.ico"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
